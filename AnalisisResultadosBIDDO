graph pie NPVD10Global if Countries=="`var'", over(ij)  
plabel(_all name, size(*1.5) color(black) format(%3.2f) gap(13))  
pie(_all, explode) plabel(_all  percent) 
legend(off)  intensity(*0.9)  
title("{it:`var'}") plotregion(lstyle(none))
